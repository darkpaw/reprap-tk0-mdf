%!PS-Adobe-3.0
%%Orientation: Landscape
%%DocumentMedia: a4 595 842 80 () ()
%%Pages: 1
%%EndComments
%%EndProlog
%%BeginSetup
  % A4, rotated 90 degrees ACW
  << /PageSize [595 842] /Orientation 3 >> setpagedevice
%%EndSetup
%%Page: 1 1
%%BeginPageSetup
  90 rotate 0 -595 translate
  %842 0 translate
  %-1 1 scale 
%%EndPageSetup
  
  
  % This page prints on standard A4 portrait paper.
  % "%%Orientation: Landscape" allows normal viewing in GSview.
  % "/Orientation 3" allows normal viewing after converting to PDF.
  % This is the preferred way to do landscape documents in 
  % PostScript, especially when converting to PDF.
  
gsave 

/inch {72 mul} def      % Convert inches->points (1/72 inch)
/mm { 72 mul 25.4 div } def
/drill { 
	3 dict begin
	/radius exch def
	/y exch def
	/x exch def
	gsave
	0.5 setlinewidth
	
	x  mm y  mm moveto
	newpath
	radius -2 mul x add mm y mm  moveto
	radius 2 mul x add mm y mm lineto
	stroke
	
	newpath
	x mm radius -2 mul y add mm  moveto
	x mm radius 2 mul y add mm lineto
	stroke
	
	newpath
	x   mm y mm radius mm 0 360 arc closepath
	stroke
	
	grestore
	end
	} def

0.5 setlinewidth
0 0 0 setrgbcolor 


% y holes SCAD
%~ for(x = [17, -17]){ 			
	%~ translate([x, 8, -1])
		%~ #cylinder(r=screw_radius, h=6);
%~ }

%~ for(x = [12, -12]){ 			
	%~ translate([x, 25, -1])
		%~ #cylinder(r=screw_radius, h=6);
%~ }

% large x spacing 34
% small x spacing 24
% y spacing 17


/Helvetica findfont 100 scalefont setfont
  30 mm 140 mm moveto
  (Y carriage base) show


/front_baseline 10 def
/rear_baseline 195 def
/mid_baseline_y rear_baseline front_baseline sub 2 div front_baseline add def

/left_baseline 73 def
/right_baseline 223 def
/mid_baseline right_baseline left_baseline sub 2 div left_baseline add def
/hole_offset 29 2 div def
/right_edge right_baseline hole_offset add 55 add def
/left_edge left_baseline hole_offset sub 55 sub def



newpath
right_edge mm 5 mm moveto
right_edge mm 220 mm lineto
closepath
stroke


newpath
left_edge mm 5 mm moveto
left_edge mm 220 mm lineto
closepath
stroke


left_edge 20 add front_baseline 10 add 2.5 drill
left_edge 20 add rear_baseline 10 sub 2.5 drill

right_edge 20 sub front_baseline 10 add 2.5 drill
right_edge 20 sub rear_baseline 10 sub 2.5 drill



newpath
left_baseline mm 5 mm moveto
left_baseline mm 220 mm lineto
closepath
stroke

newpath
right_baseline mm 5 mm moveto
right_baseline mm 220 mm lineto
closepath
stroke


newpath
mid_baseline mm 5 mm moveto
mid_baseline mm 220 mm lineto
closepath
stroke

newpath
0 mm front_baseline mm moveto
300 mm front_baseline mm lineto
closepath
stroke

newpath
0 mm rear_baseline mm moveto
300 mm rear_baseline mm lineto
closepath
stroke

newpath
0 mm mid_baseline_y mm moveto
300 mm mid_baseline_y  mm lineto
closepath
stroke





% hole spacing for LM8UU holder is 49 mm

left_baseline hole_offset sub front_baseline 1.5 drill
left_baseline hole_offset add front_baseline 1.5 drill

right_baseline hole_offset sub front_baseline 1.5 drill
right_baseline hole_offset add front_baseline 1.5 drill

left_baseline hole_offset sub rear_baseline 1.5 drill
left_baseline hole_offset add rear_baseline 1.5 drill

right_baseline hole_offset sub rear_baseline 1.5 drill
right_baseline hole_offset add rear_baseline 1.5 drill


mid_baseline 10 sub mid_baseline_y 25 add 1.5 drill
mid_baseline 20 add mid_baseline_y 25 add 1.5 drill
mid_baseline 10 sub mid_baseline_y 25 sub 1.5 drill
mid_baseline 20 add mid_baseline_y 25 sub 1.5 drill



showpage 
grestore 
%%Trailer
%%EOF
